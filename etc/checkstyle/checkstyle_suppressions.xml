<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.0//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_0.dtd">

<suppressions>
    <suppress checks="InterfaceIsType"
              files=".*.java"
            />
    <suppress checks="JavadocVariable"
              files="src/test/java/.*.java"
            />
    <suppress checks="JavadocMethod"
              files="src/test/java/.*.java"
            />
    <suppress checks="JavadocType"
              files="src/test/java/.*.java"
            />
    <suppress checks="MethodLength"
              files="src/test/java/.*.java"
            />
    <suppress checks="ImportControl"
              files="src/test/java/.*.java"
            />
    <suppress checks=".*"
              files="generated-sources/.*.java"
            />

    <suppress checks=".*"
              files="genSrc/.*.java"
            />
        
    <!-- Generated by swagger-codegen -->
    <suppress checks=".*" files="src/.*/java/io/swagger/.*/.*.java" />
    <suppress checks=".*" files="src/test.*.java" />

</suppressions>
